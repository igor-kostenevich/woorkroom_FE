{
  "name": "woorkroom-crm",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt preview",
    "postinstall": "nuxi prepare",
    "analyze": "npx nuxi analyze",
    "eslint:fix": "eslint \"**/*.{js,ts,vue,mjs}\" --ignore-pattern node_modules/ --fix",
    "prettier:write": "prettier \"**/*.{js,ts,vue,mjs}\" --write",
    "stylelint:fix": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "prepare": "node .husky/install.mjs && husky install",
    "clean": "npm cache clean --force && npx nuxi cleanup",
    "test": "vitest",
    "icons:generate": "node scripts/generate-icon-list.mjs"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,mjs}": [
      "vitest run --changed",
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{vue,css,scss}": [
      "stylelint --fix"
    ],
    "**/*.{jpeg,jpg,png,webp,avif}": "node ./.husky/compress-image.mjs",
    "**/*.svg": "node ./.husky/compress-svg.mjs"
  },
  "overrides": {
    "vue": "latest"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@floating-ui/vue": "^1.1.6",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource/barlow-semi-condensed": "^5.2.5",
    "@headlessui/vue": "^1.7.23",
    "@nuxtjs/fontaine": "^0.5.0",
    "@nuxtjs/i18n": "^10.0.3",
    "@pinia/nuxt": "^0.11.0",
    "@tanstack/vue-query": "^5.21.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "nuxt": "^3.17.5",
    "nuxt-security": "^2.2.0",
    "pinia": "^3.0.2",
    "qrcode": "^1.5.4",
    "sanitize.css": "^13.0.0",
    "socket.io-client": "^4.8.1",
    "ua-parser-js": "^2.0.3",
    "vue": "latest",
    "vue-awesome-paginate": "^1.2.0",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.0.1",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxtjs/stylelint-module": "^5.2.1",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@types/qrcode": "^1.5.5",
    "@types/vue": "^1.0.31",
    "@vueuse/core": "^13.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sass": "^1.79.6",
    "sharp": "^0.34.1",
    "stylelint": "^16.19.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^3.3.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
